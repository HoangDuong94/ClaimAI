{
  "_version": "1.65.0",
  "sap.app": {
    "id": "kfz.claims.ui.app",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.17.5",
      "toolsId": "2418c640-e786-47b9-ad94-903960b6e258"
    },
    "dataSources": {
      "mainService": {
        "uri": "/service/claims/?cb=1",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.layout": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "kfz.claims.ui.app.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "kfz.claims.ui.app.i18n.i18n"
        }
      }
    },
    "extends": {
      "extensions": {
        "sap.ui.controllerExtensions": {
          "sap.fe.templates.ObjectPage.ObjectPageController": {
            "controllerName": "kfz.claims.ui.app.ext.ClaimsOP"
          }
        },
        "sap.ui.viewExtensions": {
        "sap.fe.templates.ObjectPage.ObjectPage": {
            "HeaderContent": {
              "className": "sap.ui.core.Fragment",
              "fragmentName": "kfz.claims.ui.app.ext.HeaderUpload",
              "type": "XML"
            },
            "AfterFacet|AttachmentsTable": {
              "className": "sap.ui.core.Fragment",
              "fragmentName": "kfz.claims.ui.app.ext.AttachmentsUploadSection",
              "type": "XML"
            }
          }
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/customStyles.css"
        }
      ]
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "ClaimsList",
          "target": "ClaimsList",
          "level": 1
        },
        {
          "pattern": "Claims({key}):?query:",
          "name": "ClaimsObjectPage",
          "target": "ClaimsObjectPage",
          "level": 1
        },
        {
          "pattern": "Claims({key})/documents({docKey}):?query:",
          "name": "ClaimDocumentsObjectPage",
          "target": "ClaimDocumentsObjectPage",
          "level": 2
        }
      ],
      "targets": {
        "ClaimsList": {
          "type": "Component",
          "id": "ClaimsList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Claims",
              "variantManagement": "Page",
              "navigation": {
                "Claims": {
                  "detail": {
                    "route": "ClaimsObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  }
                }
              }
            }
          }
        },
        "ClaimsObjectPage": {
          "type": "Component",
          "id": "ClaimsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "contextPath": "/Claims",
              "editableHeaderContent": false,
              "controlConfiguration": {
                "attachments/@com.sap.vocabularies.UI.v1.LineItem": {
                  "actions": {
                    "UploadFilePicker": {
                      "text": "Anhang hochladen",
                      "press": "kfz.claims.ui.app.ext.UploadActions.onUploadAttachment",
                      "visible": "{= !${IsActiveEntity} }"
                    },
                    "DownloadAttachment": {
                      "text": "Download",
                      "press": "kfz.claims.ui.app.ext.DownloadActions.onDownloadAttachment",
                      "requiresSelection": true
                    }
                  }
                }
              },
              "navigation": {
                "documents": {
                  "detail": {
                    "route": "ClaimDocumentsObjectPage"
                  }
                }
              }
            }
          }
        },
        "ClaimDocumentsObjectPage": {
          "type": "Component",
          "id": "ClaimDocumentsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "contextPath": "/ClaimDocuments",
              "editableHeaderContent": false
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
